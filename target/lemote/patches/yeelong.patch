+#define	REG_BAT_FULLCHG_CAP_LOW		0xF781	/*  full charged capacity low byte */
+#define	REG_BAT_DESIGN_VOL_HIGH		0xF782	/*  design voltage high byte */
+#define	REG_BAT_DESIGN_VOL_LOW		0xF783	/*  design voltage low byte */
