+static unsigned int sci_poll(struct file *fp, poll_table * wait)
+{
+	int mask = 0;
+
+	/* printk("current task %p\n", current); */
+	poll_wait(fp, &(sci_device->wq), wait);
+	if (sci_device->irq_data) {
+		/* printk("current task 1 %p\n", current); */
+		mask = POLLIN | POLLRDNORM;
+	}
+
+	return mask;
+}
