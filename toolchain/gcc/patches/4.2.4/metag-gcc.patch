+
+      ival = trunc_int_for_mode (ival, SImode);
+      ops[1] = GEN_INT (ival);
+    }
+  else if (CONST_INT_P (operands[1]))
+    ops[1] = operands[1];
+  else
+    gcc_unreachable ();
+
+  metag_split_movsi_immediate (ops);
