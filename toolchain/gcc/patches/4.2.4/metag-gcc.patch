+(define_insn "fix_truncdfdi2"
+  [(set (match_operand:DI         0 "metag_fpreg_op" "=cx")
+        (fix:DI (match_operand:DF 1 "metag_fpreg_op"  "cx")))]
+  "TARGET_FPU && !metag_fpu_single"
