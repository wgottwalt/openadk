+(define_insn_and_split "tls_ldm"
+  [(set (match_operand:SI                             0 "metag_reg_nofloat_op" "=da")
+        (plus:SI (match_operand:SI                    1 "metag_pic_reg"         "a")
+                 (const:SI (unspec [(match_operand:SI 2 "tld_symbolic_operand"  "")] UNSPEC_TLSLDM))))]
