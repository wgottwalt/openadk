+  [(set (match_operand:V2SI 0 "nonimmediate_operand" "")
+        (match_operand:V2SI 1 "general_operand"      ""))]
+  "TARGET_DSP"
+  {
+    if (MEM_P (operands[0]) && !REG_P (operands[1]))
+      {
