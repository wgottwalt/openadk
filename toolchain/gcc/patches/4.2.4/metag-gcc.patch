+        if (!no_new_pseudos)
+          operands[1] = force_reg (V2SImode, operands[1]);
+      }
+
+  }
+)
+
+(define_insn "*lod_v2si"
+  [(set (match_operand:V2SI 0 "metag_datareg_op" "=d")
+        (match_operand:V2SI 1 "memory_operand"    "m"))]
+  "TARGET_DSP"
+  "GETL\\t%0, %t0, %1\\t%@ (*lod v2si rm OK)"
+  [(set_attr "memaccess" "load")])
