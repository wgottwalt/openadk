+#define PRAGMA_HWTRACE_SYNTAX_ERROR()                                                           \
+  do {                                                                                          \
+    warning (OPT_Wpragmas, "Incorrect syntax for '#pragma hwtrace_function (func|*,0|1)");      \
+    return;                                                                                     \
+  } while (0)
