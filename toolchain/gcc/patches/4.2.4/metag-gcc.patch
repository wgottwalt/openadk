+  (reg_class_size[(int) (CLASS)] == 1                                           \
+   || (CLASS) == A0_REGS                                                        \
+   || (CLASS) == A1_REGS                                                        \
+   || (CLASS) == A_REGS                                                         \
+   || (CLASS) == Ye_REGS                                                        \
+   || (CLASS) == Yf_REGS                                                        \
+   || (CLASS) == Yd_REGS                                                        \
+   || (CLASS) == Yh_REGS                                                        \
