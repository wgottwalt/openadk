+(define_expand "fix_truncsfdi2"
+  [(set (match_dup 2)
+        (float_extend:DF (match_operand:SF 1 "metag_fpreg_op" "")))
+   (set (match_operand:DI                  0 "metag_fpreg_op" "")
+        (fix:DI (match_dup 2)))]
