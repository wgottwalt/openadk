+        LSR     D1Re0, D1Ar3, #(52-32)          ! arg2 NAN ?
+        AND     D1Re0, D1Re0, #0x07FF
+        CMP     D1Re0, #0x07FF                  ! exp all 1s
+        BNE     $L11
+
+        MOV     D0Ar6, D1Ar3
+        LSL     D0Ar6, D0Ar6, #(64-52)          ! mantisa non-zero?
+        ORS     D0Ar6, D0Ar6, D0Ar4
+        MOVNZ   D0Re0, D1Ar5
+        MOVNZ   PC, D1RtP                       ! return (D1Ar3)
