+static bool
+metag_pass_in_reg (CUMULATIVE_ARGS * cum,
+                   enum machine_mode mode,
+                   tree type,
+                   bool named ATTRIBUTE_UNUSED)
+{
+  int rcum;
+
+  if (!named)
+    return false;
+
+  if (cum->narg >= MAX_METAG_PARM_BYTES)
+    return false;
+
+  rcum  = ROUND_ADVANCE_CUM (cum->narg, mode, type);
+  if (rcum >= MAX_METAG_PARM_BYTES)
+    return false; 
+
+  return true;
+}
