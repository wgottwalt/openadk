+#ifdef L_cmpsf2
+!!
+!! Compare two float(s)
+!! return -1 if <
+!!         0 if ==
+!!        +1 if >
+!!
+        .text
+        .global ___cmpsf2
+        .type   ___cmpsf2,function
+        .align  2
+___cmpsf2:
+        LSR     D1Ar3, D1Ar1, #31               ! sign1
+        LSR     D1Ar5, D0Ar2, #31               ! sign2
