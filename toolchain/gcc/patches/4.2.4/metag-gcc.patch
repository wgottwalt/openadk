+void
+metag_split_movsf_immediate (rtx operands[])
+{
+  rtx ops[2];
+
+  ops[0] = gen_rtx_REG (SImode, REGNO (operands[0]));
+  if (CONST_DOUBLE_P (operands[1]))
+    {
