+
+        CMP     D0Ar4, #0xFF                    ! exp1 == 0xFF?
+        BNE     $L1
+
+        CMP     D1Ar1, #0                       ! and mant1 == 0?
+        MOVNZ   PC, D1RtP                       ! No, Nan, return 1
+
+        B       $L2
+
+$L1:
+        CMP     D0Ar6, #0xFF                    ! exp2 == 0xFF?
+        BNE     $L2
+
