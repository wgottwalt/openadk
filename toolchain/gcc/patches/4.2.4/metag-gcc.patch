+bool
+metag_legitimate_twin_p (rtx base, rtx off, enum machine_mode mode ATTRIBUTE_UNUSED, bool strict)
+{
+  return METAG_REGS_OK_FOR_BASE_OFFSET_P (base, off, strict);
+}
