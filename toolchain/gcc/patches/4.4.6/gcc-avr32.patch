+#define IS_INTERRUPT(t)        (t & AVR32_FT_INTERRUPT)
+#define IS_NAKED(t)            (t & AVR32_FT_NAKED)
+#define IS_VOLATILE(t)         (t & AVR32_FT_VOLATILE)
+#define IS_NESTED(t)           (t & AVR32_FT_NESTED)
+#define IS_FLASHVAULT(t)       (t & AVR32_FT_FLASHVAULT)
